
<div class="container">
  <div class="row">
    <div class="col-md-5">
      <%= render 'shared/header', header: "Check out what our users have to say!" %>
      <hl>
      <div class="row">
	<ul class="nb-list">
	  <% @users.each do |user| %>
	    <li><%= link_to user.username, user_path(user.username) %></li>
	  <% end %>
	</ul>
      </div>
    </div>
  </div>
</div>

