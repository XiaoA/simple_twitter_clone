
<div class="container">
  <div class="row">
    <div class="col-sm-6">
      <%= render 'shared/header', header: "Check out what our users have to say!" %>
      <hl>
      <div class="row">
	<!-- <ul class="nb-list"> -->
	<ul class ="list-group">
	  <% @users.each do |user| %>
	    <li class="list-group-item"><%= link_to user.username, user_path(user.username) %></li>
	  <% end %>
	</ul>
      </div>
    </div>
  </div>
</div>

